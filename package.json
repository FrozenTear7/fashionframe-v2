{
  "name": "fashionframe-v2",
  "version": "1.0.0",
  "description": "Social hub for sharing Warframe fashion setups",
  "main": "index.js",
  "repository": "https://github.com/FrozenTear7/fashionframe-v2.git",
  "author": "Pawe≈Ç Mendroch <pawelmendroch7@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-rewire-ts": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "node build/src/index.js",
    "dev": "ts-node-dev src/index.ts",
    "lint": "eslint --ext .ts . --fix",
    "test": "jest --passWithNoTests --no-cache --coverage",
    "test:coverage": "jest --passWithNoTests --no-cache --silent --coverage",
    "test:badges": "yarn test:coverage && jest-coverage-badges --input ./coverage/coverage-summary.json --output ./badges"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:badges && git add ./badges"
    }
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "eslint --fix",
      "git add ."
    ]
  }
}
